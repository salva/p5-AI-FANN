#!/usr/bin/perl

my @names;
while(<DATA>) {
	chomp;
	next if /^\s*(?:#.*)?$/;
	push @names, $_
}

my $n = @names;
my $n2 = $n*2;

print "static char * const my_constant_names[] = {\n";
for (@names) {
	print qq(    "$_",\n)
}
print qq(    0,\n);
print "};\n";
print "static const unsigned int my_constant_values[] = {\n";
for (@names) {
	print qq(    $_,\n)
}
print "};\n";

__DATA__

